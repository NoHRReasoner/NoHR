            </CNode>
        </HSNode>
        <CNode>
            <Component label="NoHR rules">
                <Property id="pluginId" value="pt.unl.fct.di.novalincs.nohr.plugin.RulesViewComponent"/>
            </Component>
        </CNode>
    </VSNode>
