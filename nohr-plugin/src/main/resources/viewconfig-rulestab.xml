<layout>
    <VSNode splits="0.3 0.7">
        <HSNode splits="0.5 0.5">
            <CNode>
                <Component label="Asserted class hierarchy">
                    <Property id="pluginId" value="org.protege.editor.owl.OWLAssertedClassHierarchy"/>
                </Component>
                <Component label="Inferred class hierarchy">
                    <Property id="pluginId" value="org.protege.editor.owl.InferredOWLClassHierarchy"/>
