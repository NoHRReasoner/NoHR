#!/bin/bash

RUNS=$1
QUERYFILE=$2
U1=$3
U2=$4
U3=$5

source ../env

for u in {${U1},${U2},${U3}}; do
	echo "Uni${u}"
	for ((i=1; i<= $RUNS; i++)); do
		echo "Run ${i}"
		sh ../runclass "benchmark.LubmTest" "--profile QL --warmup-file Uni1.owl --data-file Uni${u}.owl --queries-file ${QUERYFILE}"\
		 	2>&1 | tee logs/ql${u}_${i}.txt
		mv queries.csv ./report/nohr-queries${u}_${i}.csv
		mv loading.csv ./report/nohr-loading${u}_${i}.csv
	done
done


for i in {${U1},${U2},${U3}}; do
	echo "Uni${i}"
	${JAVA} -Xmx${XMX}\
     -DentityExpansionLimit=100000000 \
		 -jar .pellet.jar Uni${i}.owl LUBM \
		 | tee ./report/pellet${i}.txt
done
