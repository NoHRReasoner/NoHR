#!/bin/sh

RUNS=$1
QUERYFILE=$2
UNIVS="${@:3:$#}" 

echo "QL"

for ((i=1; i<=$RUNS; i++)); do
	echo "RUN ${i}" 
	sh ../.runclass "benchmark.LubmTest" "--profile QL --data-dir data --queries-file ${QUERYFILE} --univs-list ${UNIVS}"\
		 2>&1 | tee logs/ql${i}.txt
	mv queries.csv ./report/queries/ql${i}.csv
	mv loading.csv ./report/loading/ql${i}.csv
done

echo "EL"

for ((i=1; i<=$RUNS; i++)); do
	echo "RUN ${i}"
  sh ../.runclass "benchmark.LubmTest" "--profile EL --data-dir data --queries-file ${QUERYFILE} --univs-list ${UNIVS}"\
		 2>&1 | tee logs/el${i}.txt
	mv queries.csv ./report/queries/el${i}.csv
	mv loading.csv ./report/loading/el${i}.csv
done
